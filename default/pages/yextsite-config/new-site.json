{
    "$id": "${{siteId}}",
    "$schema": "https://schema.yext.com/config/pages/yextsite-config/v1",
    "domain": "${{domainName}}",
    "siteName": "${{siteName}}",
    "repoConfig": "${{repoId}}",
    "siteSettings": {
      "autoDeploy": true,
      "autoStage": true,
      "autoPublish": true,
      "pullRequestsEnabled": false,
      "stagingCredentials": {
        "password": "",
        "username": ""
      }
    },
    "branches": [
      {
        "name": "main",
        "isProductionBranch": true,
        "env": [
          {
            "siteId": "${{siteId}}"
          }
        ]
      }
    ]
  }
  